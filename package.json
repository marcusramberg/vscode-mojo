{
    "name": "mojolicious",
    "displayName": "Mojolicious",
    "description": "VS Code support for Mojolicious Template syntax",
    "version": "1.8.0",
    "publisher": "kraih",
    "repository": {
        "type": "git",
        "url": "https://github.com/kraih/vscode-mojo"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "license": "MIT",
    "categories": [
        "Languages"
    ],
    "keywords": [
        "mojolicious",
        "perl",
        "template"
    ],
    "icon": "logo.png",
    "galleryBanner": {
        "color": "#ffffff",
        "theme": "light"
    },
    "contributes": {
        "languages": [
            {
                "id": "mojolicious",
                "aliases": [
                    "Mojolicious",
                    "ep",
                    "epl"
                ],
                "extensions": [
                    ".ep",
                    ".epl",
                    ".html.ep",
                    ".html.epl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "injectInto": ["source.perl"],
                "scopeName": "source.perl.mojolicious",
                "path": "./syntaxes/Perl+Mojolicious.json",
                "embeddedLanguages": {
                  "text.html.mojolicious": "mojolicious"
                }
            },
            {
                "language": "mojolicious",
                "scopeName": "text.html.mojolicious",
                "path": "./syntaxes/Mojolicious.json"
            }
        ]
    },
    "dependencies": {
        "vsce": "^1.35.0"
    }
}
